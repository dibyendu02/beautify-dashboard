{
  "name": "beautify-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3003",
    "build": "next build",
    "start": "next start -p 3003",
    "lint": "eslint --fix --ext .ts,.tsx .",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:unit": "jest --selectProjects unit",
    "test:integration": "jest --selectProjects integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:coverage": "jest --coverage",
    "test:coverage:unit": "jest --selectProjects unit --coverage",
    "test:coverage:integration": "jest --selectProjects integration --coverage",
    "test:watch": "jest --watch --selectProjects unit",
    "test:ci": "jest --ci --coverage --runInBand --selectProjects unit integration && playwright test",
    "test:all": "npm run test:ci",
    "playwright:install": "playwright install",
    "test:smoke": "playwright test --grep smoke",
    "test:visual": "playwright test --grep visual"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-slot": "^1.2.3",
    "@reduxjs/toolkit": "^2.9.0",
    "@shadcn/ui": "^0.0.4",
    "axios": "^1.11.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.543.0",
    "next": "15.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "react-hot-toast": "^2.6.0",
    "react-redux": "^9.2.0",
    "recharts": "^3.2.0",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.3.1",
    "yup": "^1.7.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
